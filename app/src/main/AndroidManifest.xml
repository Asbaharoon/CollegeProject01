<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jesan.collegeproject01" >
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudentContactActivity"
            android:label="Add Info"/>
        <activity android:name=".StudentDataListActivity"
            android:label="Attendence"/>
       <activity android:name=".FeatureWork"
           android:label="Logged In"/>
        <activity android:name=".TeacherSignUp"
            android:label="Sign Up"/>
        <activity android:name=".StudentReportListActivity"
            android:label="Report"/>
        <activity android:name=".SettingsActivity"
            android:label="Settings"/>
        <activity android:name=".SearchContactActivity"
            android:label="Search"/>
        <activity android:name=".DataBaseCSVExport"
            android:label="CSV Export"/>


    </application>

</manifest>
